{
    "name": "mdoutoure/nicetruc",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "": "src/",
            "SymfonyStandard\\": "app/SymfonyStandard/"
        }
    },
    "require": {
        "php": "5.6",
        "symfony/symfony": "2.8.*",
        "doctrine/orm": "^2.4.8",
        "doctrine/dbal": "^2.4.8",
        "doctrine/doctrine-bundle": "~1.4",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~4.0",
        "sensio/framework-extra-bundle": "~3.0,>=3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "sylius/resource-bundle": "^0.14.0",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "doctrine/doctrine-fixtures-bundle": "dev-master",
        "vich/uploader-bundle": "^0.14.0",
        "srio/rest-upload-bundle": "^2.0",
        "nelmio/api-doc-bundle": "^2.9",
        "friendsofsymfony/jsrouting-bundle": "^1.5",
        "knplabs/knp-gaufrette-bundle": "0.2.*",
        "aws/aws-sdk-php": "^3.11",
        "aws/aws-sdk-php-symfony": "^1.1",
        "friendsofsymfony/elastica-bundle": "^3.1",
        "lexik/jwt-authentication-bundle": "^1.3",
        "white-october/pagerfanta-bundle": "^1.0",
        "knplabs/knp-paginator-bundle": "^2.5",
        "gesdinet/jwt-refresh-token-bundle": "^0.1.3"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3"
    },
    "scripts": {
        "compile": [
            "php ./app/console doctrine:schema:update --force",
            "php ./app/console fos:elastica:populate"
        ],
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "npm install bower",
            "npm install gulp",
            "npm install gulp-concat",
            "npm install del",
            "npm install gulp-concat-css",
            "./node_modules/.bin/bower install",
            "./node_modules/.bin/gulp"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "npm install bower",
            "npm install gulp",
            "npm install gulp-concat",
            "npm install del",
            "npm install gulp-concat-css",
            "./node_modules/.bin/bower install",
            "./node_modules/.bin/gulp"

        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
